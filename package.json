        "@angular/cli": "^18.2.1",
        "@angular/compiler-cli": "^18.2.1",
        "@emoji-mart/data": "^1.2.1",
        "@types/express": "^4.17.21",
        "@types/feather-icons": "^4.29.4",
        "@types/jasmine": "^5.1.4",
        "@types/leaflet": "^1.9.12",
        "autoprefixer": "^10.4.20",
        "emoji-mart": "^5.6.0",
        "howler": "^2.2.4",
        "jasmine-core": "^5.2.0",
        "karma": "^6.4.4",
