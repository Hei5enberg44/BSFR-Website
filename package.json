{
    "name": "website-front",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "serve:ssr:frontend": "node dist/frontend/server/server.mjs"
    },
    "private": true,
    "prettier": {
        "trailingComma": "none",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "bracketSpacing": true,
        "bracketSameLine": true,
        "arrowParens": "always",
        "overrides": [
            {
                "files": "*.html",
                "options": {
                    "tabWidth": 2
                }
            }
        ]
    },
    "dependencies": {
        "@angular/animations": "^18.2.1",
        "@angular/cdk": "^18.2.1",
        "@angular/common": "^18.2.1",
        "@angular/compiler": "^18.2.1",
        "@angular/core": "^18.2.1",
        "@angular/forms": "^18.2.1",
        "@angular/platform-browser": "^18.2.1",
        "@angular/platform-browser-dynamic": "^18.2.1",
        "@angular/platform-server": "^18.2.1",
        "@angular/router": "^18.2.1",
        "@angular/ssr": "^18.2.1",
        "@asymmetrik/ngx-leaflet": "^18.0.1",
        "@ngx-translate/core": "^15.0.0",
        "@ngx-translate/http-loader": "^8.0.0",
        "esri-leaflet-vector": "^4.2.4",
        "express": "^4.19.2",
        "feather-icons": "^4.29.2",
        "leaflet": "^1.9.4",
